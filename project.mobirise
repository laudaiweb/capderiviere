          "_customHTML": "<section class=\"mbr-section features2 hotelm4_features2\" group=\"Features\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\n\n\t<mbr-parameters>\n\t\t<!-- Block parameters controls (Blue \"Gear\" panel) -->\n\t\t<header>Size</header>\n\t\t<input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"4\">\n\t\t<input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"4\">\n\t\t<input type=\"range\" inline title=\"Cards Count\" name=\"cardsAmount\" min=\"1\" max=\"5\" step=\"1\" value=\"5\">\n\t\t<header>Show / Hide</header>\n\t\t<input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Underline\" name=\"showUnderline\" checked condition=\"showSubtitle\">\n\t\t<input type=\"color\" title=\"Underline Color\" name=\"underlineColor\" value=\"#878787\" condition=\"showUnderline && showSubtitle\">\n\n\t\t<input type=\"checkbox\" title=\"Scale Image On Hover\" name=\"scaleImg\" checked>\n\t\t<input type=\"checkbox\" title=\"Card Title\" name=\"showCardTitle\" checked condition=\"cardsAmount > 1\">\n\t\t<input type=\"checkbox\" title=\"Price\" name=\"showPrice\" condition=\"cardsAmount > 1\">\n\t\t<input type=\"color\" title=\"Price BG Color\" name=\"priceBg\" value=\"#c19b76\" condition=\"showPrice &amp;&amp; cardsAmount > 1\">\n\n\t\t<header>First Card Settings</header>\n\t\t<input type=\"checkbox\" title=\"Show Title\" name=\"showFirstTitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Show Info\" name=\"showInfo\" checked>\n\t\t<input type=\"checkbox\" title=\"Show Text\" name=\"showFirstText\" checked>\n\t\t<input type=\"checkbox\" title=\"Show Buttons\" name=\"showButtons\" checked>\n\t\t<input type=\"color\" title=\"Background Color\" name=\"firstBg\" value